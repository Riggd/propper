{
  "name": "propper",
  "version": "1.0.0",
  "description": "AI-powered design-to-code bridge for Figma component handoff",
  "private": true,
  "workspaces": ["proxy", "plugin", "cli"],
  "scripts": {
    "dev": "npm run dev -w proxy -w plugin",
    "dev:proxy": "npm run dev --workspace=proxy",
    "dev:plugin": "npm run dev --workspace=plugin",
    "build:proxy": "npm run build --workspace=proxy",
    "build:plugin": "npm run build --workspace=plugin",
    "build:cli": "npm run build --workspace=cli",
    "build": "npm run build -w proxy -w plugin && npm run build:cli"
  }
}
